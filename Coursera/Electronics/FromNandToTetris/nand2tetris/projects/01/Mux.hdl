/** 
 * Multiplexor. If sel==1 then out=b else out=a.
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    And(a=b, b=sel, out=aux1);
    Not(in=sel, out=aux2);
    And(a=a, b=aux2, out=aux3);
    Or(a=aux3, b=aux1, out=out);
}